doctype html
html
  head
    title Contacts List
    link(rel="stylesheet", href="/css/style.css")
  body
    h1 HubSpot Contacts
    a(href="/") Return to homepage
    table
      thead
        tr
          th First Name
          th Last Name
          th Email
      tbody
        each contact in contacts
          tr
            td= contact.properties.firstname
            td= contact.properties.lastname
            td= contact.properties.email